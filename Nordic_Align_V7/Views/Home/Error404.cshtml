@{
    ViewData["Title"] = "Ooops! Page was not found";
}


<section id="error">

    <div class="container">
        <img src="~/images/error.jpg" alt="404 error" />

        <div class="content">
            <h2 class="title">Ooops!</h2>
            <p>The page you are looking for is not available.</p>
            <a class="btn-theme-s" asp-action="Index" asp-controller="Home"><i class="fa-solid fa-house"></i> Go to homepage</a>
        </div>
        
    </div>
</section>
